{
  "name": "@apexwoot/mr-scraper",
  "version": "1.0.23",
  "description": "TypeScript library for scraping LinkedIn person profiles using Playwright",
  "type": "module",
  "main": "./dist/node/index.js",
  "module": "./dist/node/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./dist/bun/index.js",
      "node": "./dist/node/index.js",
      "default": "./dist/node/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "linkedin",
    "scraper",
    "playwright",
    "typescript",
    "web-scraping",
    "linkedin-api",
    "linkedin-scraper"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apex-woot/mr-scraper.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/apex-woot/mr-scraper/issues"
  },
  "homepage": "https://github.com/apex-woot/mr-scraper#readme",
  "author": "apexwoot",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "playwright": "^1.58.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "test": "bun test",
    "scrape": "bun run index.ts",
    "scrape:details": "bun run examples/scrape_details_section.ts",
    "scrape:education": "bun run examples/scrape_education_only.ts",
    "scrape:publications": "bun run examples/scrape_publications_only.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build && bun test"
  }
}
